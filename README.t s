WDS.sampleResult.sampleStart()

var driver = WDS.browser
driver.get('http://www.google.com')

// 获取当前页面标题
var pageTitle = driver.getTitle()
WDS.log.info("Page Title: " + pageTitle)

if (pageTitle === 'Google') {
    WDS.log.info("Successfully navigated to the target page.")
} else {
    WDS.log.error("Failed to navigate to the target page.")
}

WDS.sampleResult.sampleEnd()
