WDS.sampleResult.sampleStart()

var driver = WDS.browser
driver.get('http://www.example.com')

// 定位输入框元素
var searchBox = driver.findElement(org.openqa.selenium.By.xpath("//input[@name='q']"))

// 使用Actions类模拟输入
var actions = new org.openqa.selenium.interactions.Actions(driver)
actions.click(searchBox).sendKeys("123").perform()

WDS.sampleResult.sampleEnd()
